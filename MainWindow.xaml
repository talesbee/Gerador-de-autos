                <Label x:Name="lbNome_Copy" Content="Filiação (Pessoa Fisica) Mãe" HorizontalAlignment="Left" Margin="10,76,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txFiliacao" HorizontalAlignment="Left" Margin="22,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="299" FontSize="14" TabIndex="4"/>
                <Label x:Name="lbNome_Copy1" Content="Atividade" HorizontalAlignment="Left" Margin="325,76,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txAtividade" HorizontalAlignment="Left" Margin="339,104,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="299" FontSize="14" TabIndex="5"/>
                <Label x:Name="lbGeralLivre" Content="Livre" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#FFFD0000" FontWeight="Bold" FontSize="14" Visibility="Hidden"/>
                <CheckBox x:Name="cbGeralLivre" Content="Mostrar #" HorizontalAlignment="Left" Margin="50,7,0,0" VerticalAlignment="Top" Foreground="Red" Visibility="Hidden"/>

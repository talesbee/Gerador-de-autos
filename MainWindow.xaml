                <Label x:Name="lbNumero_Copy22" Content="Total Reposição Florestal:" HorizontalAlignment="Left" Margin="394,281,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txNotTRep" HorizontalAlignment="Left" Margin="406,309,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" FontSize="14" TabIndex="9"/>
                <Button x:Name="btInfraPreencher_Copy" Content="Gerar Texto" HorizontalAlignment="Left" Height="20" Margin="589,287,0,0" VerticalAlignment="Top" Width="76" FontFamily="Times New Roman" Click="buscarModelos" TabIndex="10"/>
            </Grid>
        </TabItem>
        <TabItem x:Name="tbApreensao" Header="Termo de Apreensão" HorizontalAlignment="Center" Height="20" VerticalAlignment="Center" FontFamily="Times New Roman" Visibility="Visible">
            <Grid Background="#FFE5E5E5">
                <Label x:Name="lbApreLivre" Content="Livre" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#FFFD0000" FontWeight="Bold" FontSize="14" Visibility="Hidden"/>
                <CheckBox x:Name="cbApreLivre" Content="Mostrar #" HorizontalAlignment="Left" Margin="50,7,0,0" VerticalAlignment="Top" Foreground="Red" Visibility="Hidden"/>
            </Grid>
        </TabItem>
        <TabItem x:Name="tbDeposito" Header="Termo de Depósito" HorizontalAlignment="Center" Height="20" VerticalAlignment="Center" Width="110" FontFamily="Times New Roman" Visibility="Visible">

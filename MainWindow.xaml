                <Button x:Name="btInfraPreencher" Content="Gerar Texto" HorizontalAlignment="Left" Height="20" Margin="187,167,0,0" VerticalAlignment="Top" Width="76" FontFamily="Times New Roman" Click="buscarModelos" TabIndex="1"/>
                <Label x:Name="lbNumero_Copy3" Content="Valor da Multa:" HorizontalAlignment="Left" Margin="369,32,0,0" VerticalAlignment="Top" FontSize="16"/>
                <Label x:Name="lbNumero_Copy4" Content="Valor por HA:" HorizontalAlignment="Left" Margin="384,60,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txInfraValorHA" HorizontalAlignment="Left" Margin="399,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="93" FontSize="14" TabIndex="4" Text="5000"/>
                <Label x:Name="lbNumero_Copy5" Content="Total (automático):" HorizontalAlignment="Left" Margin="493,60,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txInfraMulta" HorizontalAlignment="Left" Margin="507,88,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" FontSize="14" TabIndex="5"/>
                <Label x:Name="lbNumero_Copy6" Content="Total por Extenso:" HorizontalAlignment="Left" Margin="385,106,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txInfraMultaExtenso" HorizontalAlignment="Left" Margin="399,134,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="315" FontSize="14" TabIndex="6"/>
                <Label x:Name="lbNumero_Copy7" Content="Área Exploração:" HorizontalAlignment="Left" Margin="22,150,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txInfraAreaExploracao" HorizontalAlignment="Left" Margin="37,178,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="94" FontSize="14" TabIndex="3" DataContextChanged="calculoMulta"/>
                <Label x:Name="lbNumero_Copy8" Content="Ocorrência:" HorizontalAlignment="Left" Margin="22,198,0,0" VerticalAlignment="Top" FontSize="16"/>
                <CheckBox x:Name="cbInfraDesmate" Content="Desmatamento" HorizontalAlignment="Left" Margin="154,205,0,0" VerticalAlignment="Top"/>
                <Label x:Name="lbNumero_Copy9" Content="Dispositivos legais infringidos:" HorizontalAlignment="Left" Margin="385,156,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txInfraDispositivosInfri" HorizontalAlignment="Left" Margin="399,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="315" FontSize="14" TabIndex="7" Text="ART. 70 DA LEI FEDERAL Nº 9.605/1998 C/C ART. 50 DO DECRETO FEDERAL Nº 6.514/2008."/>
                <Label x:Name="lbNumero_Copy10" Content="Descrição do valor da multa:" HorizontalAlignment="Left" Margin="385,219,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="lbInfraDescriMulta" HorizontalAlignment="Left" Margin="399,252,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="315" Height="68" UndoLimit="0" TabIndex="8"/>
                <Label x:Name="lbInfraLivre" Content="Livre" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#FFFD0000" FontWeight="Bold" FontSize="14" Visibility="Hidden"/>
                <CheckBox x:Name="cbInfraLivre" Content="Mostrar #" HorizontalAlignment="Left" Margin="50,7,0,0" VerticalAlignment="Top" Foreground="Red" Visibility="Hidden"/>
            </Grid>
        </TabItem>
        <TabItem x:Name="tbInspecao" Header="Auto de Inspeção" HorizontalAlignment="Center" Height="20" VerticalAlignment="Center" FontFamily="Times New Roman" Visibility="Visible">

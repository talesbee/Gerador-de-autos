                <Label x:Name="lbCnpjCpf" Content="CNPJ/CPF" HorizontalAlignment="Left" Margin="326,30,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txCnpjCpf" HorizontalAlignment="Left" Margin="339,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="180" FontSize="14" TabIndex="1"/>
                <Label x:Name="lbData" Content="Data" HorizontalAlignment="Left" Margin="523,30,0,0" VerticalAlignment="Top" FontSize="16"/>
                <Label x:Name="lbEnderecoComercial" Content="Endereço do Empreendimento" HorizontalAlignment="Left" Margin="10,122,0,0" VerticalAlignment="Top" FontSize="16"/>

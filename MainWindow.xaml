                <Label x:Name="lbNumero_Copy15" Content="Floresta" HorizontalAlignment="Left" Margin="14,208,0,0" VerticalAlignment="Top" FontSize="16"/>
                <Label x:Name="lbNumero_Copy16" Content="Área Desmate:" HorizontalAlignment="Left" Margin="33,234,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txNotFlorDesmate" HorizontalAlignment="Left" Margin="45,262,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" FontSize="14" TabIndex="4"/>
                <Label x:Name="lbNumero_Copy17" Content="Reposição Florestal:" HorizontalAlignment="Left" Margin="33,281,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txNotFlorRep" HorizontalAlignment="Left" Margin="45,309,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" FontSize="14" TabIndex="5"/>
                <Label x:Name="lbNumero_Copy18" Content="Cerrado" HorizontalAlignment="Left" Margin="195,208,0,0" VerticalAlignment="Top" FontSize="16"/>
                <Label x:Name="lbNumero_Copy19" Content="Área Desmate:" HorizontalAlignment="Left" Margin="214,234,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txNotCerDesmate" HorizontalAlignment="Left" Margin="226,262,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" FontSize="14" TabIndex="6"/>
                <Label x:Name="lbNumero_Copy20" Content="Reposição Florestal:" HorizontalAlignment="Left" Margin="214,281,0,0" VerticalAlignment="Top" FontSize="16"/>

                <TextBox x:Name="txEnderecoCorrespondencia" HorizontalAlignment="Left" Margin="22,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="301" FontSize="14" TabIndex="13"/>
                <Label x:Name="lbMunicipioCorrespondencia" Content="Município" HorizontalAlignment="Left" Margin="330,214,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txMunicipioCorrespondencia" HorizontalAlignment="Center" Margin="0,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="110" FontSize="14" TabIndex="14"/>
                <Label x:Name="lbUfCorrespondencia" Content="UF" HorizontalAlignment="Left" Margin="456,214,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txUfCorrespondencia" HorizontalAlignment="Left" Margin="468,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="44" FontSize="14" TabIndex="15"/>
                <Label x:Name="lbCEP" Content="CEP" HorizontalAlignment="Left" Margin="524,214,0,0" VerticalAlignment="Top" FontSize="16"/>
                <TextBox x:Name="txCEP" HorizontalAlignment="Left" Margin="538,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="108" FontSize="14" TabIndex="16"/>
                <Label x:Name="lbTelefone" Content="Telefone" HorizontalAlignment="Left" Margin="9,260,0,0" VerticalAlignment="Top" FontSize="16"/>
